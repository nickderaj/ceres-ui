{
  "name": "ceres-ui",
  "version": "0.1.2",
  "type": "module",
  "description": "A dark-themed UI component library for blog and content sites, built with React and CSS custom properties.",
  "main": "dist/ceres-ui.cjs.js",
  "module": "dist/ceres-ui.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ceres-ui.es.js",
      "require": "./dist/ceres-ui.cjs.js"
    },
    "./css": "./dist/ceres-ui.css"
  },
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc -p tsconfig.build.json && vite build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "release": "pnpm run typecheck && pnpm test && npm publish",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx,css,md,json}": "prettier --write"
  },
  "peerDependencies": {
    "katex": ">=0.16.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-markdown": ">=10.0.0",
    "react-syntax-highlighter": ">=15.0.0",
    "recharts": ">=2.0.0",
    "rehype-katex": ">=7.0.0",
    "rehype-raw": ">=7.0.0",
    "remark-gfm": ">=4.0.0",
    "remark-math": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "react-markdown": {
      "optional": true
    },
    "react-syntax-highlighter": {
      "optional": true
    },
    "recharts": {
      "optional": true
    },
    "rehype-katex": {
      "optional": true
    },
    "rehype-raw": {
      "optional": true
    },
    "remark-gfm": {
      "optional": true
    },
    "remark-math": {
      "optional": true
    },
    "katex": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "katex": "^0.16.27",
    "prettier": "^3.4.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "recharts": "^3.7.0",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18",
    "lint-staged": "^15.4.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "catppuccin",
    "dark-theme",
    "blog",
    "markdown"
  ]
}
